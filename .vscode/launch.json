{
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch Debug",
            "program": "${workspaceFolder}/build/Debug/tack",
            "args": [ "source.str", "-A", "-D" ],
            // "args": [ "test/test_sort.str", "-A", "-D" ],
            // "args": [ "test/test_closure.str", "-A", "-D" ],
            "preLaunchTask": "Build - Debug",
            "cwd": "${workspaceFolder}"
        }, {
            "type": "lldb",
            "request": "launch",
            "name": "Launch Release",
            "program": "${workspaceFolder}/build/Release/tack",
            "args": [ "source.str", "-D", "-A" ],
            "preLaunchTask": "Build - Release",
            "cwd": "${workspaceFolder}"
        }
    ]
}
