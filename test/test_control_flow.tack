fn() {
    print("fizzbuzz test")
    for i in 1, 31 {
        if i % 3 == 0 {
            if i % 5 == 0 {
                print("fizzbuzz")
            } else {
                print("fizz")
            }
        } else if i % 5 == 0 {
            print("buzz")
        } else {
            print(i)
        }
    }
}()

