fn test_closure() {
    let i = 100
    if true {
        let i = 200
        if false {
            let i = 300
        } else {
            i = i + 1
            fn write() {
                i = i + 1
            }
            write()
            fn read() {
                return fn() {
                    return i
                }
            }
            write()
            return read
        }
    }
}

let a = test_closure()()()
print("Expect 203: ", a)
