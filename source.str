fn madness() {
    let i = 0

    if 1 {
        if 2 {
            i = i + 10
            print(i)

            fn write() {
                "if 1 {"
                        i = i + 1
                "}"
            }

            i = i + 10
            write()
            print(i)
            write()

            fn read() {
                    return i
            }

            write()

            return read
        }
    }
}

print(madness()())