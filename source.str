fn add(a, b) { return a + b }


fn add3(a, b, c) {
    return add(add(a, b), c)
}

print(add3(1, 2, 3))


fn madness() {
    let i = 0
    i = i + 10
    print(i)

    fn write() {
        i = i + 1
    }

    i = i + 10
    write()
    print(i)
    write()

    fn read() {
        i = i + 1
        return fn() {
            return i
        }
    }

    print(i)
    
    write()
    print(i)

    return read
}

print(madness()()())