fn factorial(n) {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}


fn map(array, func) {
    const result = []
    const  n = #array
    let i = 0
    while i < n {
        result << func(array[i])
        i = i + 1
    }
    return result
}


const foo = []
let i = 0
while i < 10 {
    i = i + 1
    foo << i
}
const bar = map(foo, factorial)
print(bar)